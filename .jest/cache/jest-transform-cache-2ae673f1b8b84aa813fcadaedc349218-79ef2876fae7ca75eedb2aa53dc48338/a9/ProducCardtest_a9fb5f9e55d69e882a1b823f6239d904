var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _ProductCard = require("../ProductCard");
var _jsxRuntime = require("react/jsx-runtime");
describe('ModalComponent', function () {
  it('ProductCard renders correctly with name and ID', function () {
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ProductCard.ProductCard, {
        name: "Test Product",
        id: "123",
        goToDetails: function goToDetails() {}
      })),
      getByText = _render.getByText;
    var productName = getByText('Test Product');
    var productId = getByText('ID: 123');
    expect(productName).toBeDefined();
    expect(productId).toBeDefined();
  });
  it('ProductCard calls goToDetails on press', function () {
    var mockGoToDetails = jest.fn();
    var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ProductCard.ProductCard, {
        name: "Test Product",
        id: "123",
        goToDetails: mockGoToDetails
      })),
      getByTestId = _render2.getByTestId;
    var productCard = getByTestId('product-card');
    _reactNative.fireEvent.press(productCard);
    expect(mockGoToDetails).toHaveBeenCalled();
  });
});
