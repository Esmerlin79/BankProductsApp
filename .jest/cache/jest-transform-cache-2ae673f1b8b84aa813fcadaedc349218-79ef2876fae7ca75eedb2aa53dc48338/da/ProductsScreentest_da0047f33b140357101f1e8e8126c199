var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _ProductsScreen = require("../ProductsScreen");
var _jsxRuntime = require("react/jsx-runtime");
describe('ProductsScreen', function () {
  it('renders correctly', function () {
    var navigation = {
      navigate: jest.fn()
    };
    var route = {
      params: {}
    };
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ProductsScreen.ProductsScreen, {
        navigation: navigation,
        route: route
      })),
      getByText = _render.getByText;
    expect(getByText('BANCO PICHINCHA')).toBeTruthy();
  });
  it('updates search input correctly', function () {
    var navigation = {
      navigate: jest.fn()
    };
    var route = {
      params: {}
    };
    var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ProductsScreen.ProductsScreen, {
        navigation: navigation,
        route: route
      })),
      getByPlaceholderText = _render2.getByPlaceholderText;
    var searchInput = getByPlaceholderText('Search...');
    _reactNative.fireEvent.changeText(searchInput, 'Texto de búsqueda');
    expect(searchInput.props.value).toBe('Texto de búsqueda');
  });
  it('navigates to ProductRegisterScreen when clicking on the "Agregar" button', function () {
    var navigation = {
      navigate: jest.fn()
    };
    var route = {
      params: {}
    };
    var _render3 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ProductsScreen.ProductsScreen, {
        navigation: navigation,
        route: route
      })),
      getByText = _render3.getByText;
    _reactNative.fireEvent.press(getByText('Agregar'));
    expect(navigation.navigate).toHaveBeenCalledWith('ProductRegisterScreen', {});
  });
});
