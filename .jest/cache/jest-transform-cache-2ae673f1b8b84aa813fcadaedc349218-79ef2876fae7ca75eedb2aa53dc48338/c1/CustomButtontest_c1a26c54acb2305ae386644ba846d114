var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _CustomButton = require("../CustomButton");
var _jsxRuntime = require("react/jsx-runtime");
describe('CustomButton', function () {
  it('should render correctly', function () {
    var mockOnPress = jest.fn();
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_CustomButton.CustomButton, {
        title: "Test Button",
        onPress: mockOnPress,
        backgroundColor: "blue",
        color: "white"
      })),
      getByText = _render.getByText,
      getByTestId = _render.getByTestId;
    var buttonText = getByText('Test Button');
    expect(buttonText).toBeTruthy();
    var buttonContainer = getByTestId('custom-button-container');
    var backgroundColor = buttonContainer.props.style.backgroundColor;
    expect(backgroundColor).toBe('blue');
    _reactNative.fireEvent.press(buttonContainer);
    expect(mockOnPress).toHaveBeenCalledTimes(1);
  });
});
