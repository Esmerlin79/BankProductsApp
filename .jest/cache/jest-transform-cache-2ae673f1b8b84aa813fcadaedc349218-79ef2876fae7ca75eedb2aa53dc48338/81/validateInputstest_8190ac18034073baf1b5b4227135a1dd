var _validateInputs = require("../validateInputs");
describe('validateInputs', function () {
  it('should return false for valid ID', function () {
    expect((0, _validateInputs.validateInputs)('ID', 'validId')).toBe(false);
  });
  it('should return true for invalid ID (length < 3)', function () {
    expect((0, _validateInputs.validateInputs)('ID', '12')).toBe(true);
  });
  it('should return true for invalid ID (length > 10)', function () {
    expect((0, _validateInputs.validateInputs)('ID', 'veryLongInvalidId')).toBe(true);
  });
  it('should return false for valid name', function () {
    expect((0, _validateInputs.validateInputs)('name', 'Valid Name')).toBe(false);
  });
  it('should return true for invalid name (length < 5)', function () {
    expect((0, _validateInputs.validateInputs)('name', '123')).toBe(true);
  });
  it('should return true for invalid name (length > 100)', function () {
    var longName = 'a'.repeat(101);
    expect((0, _validateInputs.validateInputs)('name', longName)).toBe(true);
  });
  it('should return false for valid description', function () {
    expect((0, _validateInputs.validateInputs)('description', 'Valid Description')).toBe(false);
  });
  it('should return true for invalid description (length < 10)', function () {
    expect((0, _validateInputs.validateInputs)('description', 'ShortDesc')).toBe(true);
  });
  it('should return true for invalid description (length > 200)', function () {
    var longDescription = 'a'.repeat(201);
    expect((0, _validateInputs.validateInputs)('description', longDescription)).toBe(true);
  });
  it('should return false for valid logo', function () {
    expect((0, _validateInputs.validateInputs)('logo', 'validLogo')).toBe(false);
  });
  it('should return true for invalid logo (empty)', function () {
    expect((0, _validateInputs.validateInputs)('logo', '')).toBe(true);
  });
  it('should return false for valid release date', function () {
    var currentDate = new Date();
    var validDate = new Date(currentDate.getTime() + 86400000);
    expect((0, _validateInputs.validateInputs)('releaseDate', validDate.toISOString())).toBe(false);
  });
  it('should return true for invalid release date (in the past)', function () {
    var currentDate = new Date();
    var pastDate = new Date(currentDate.getTime() - 86400000);
    expect((0, _validateInputs.validateInputs)('releaseDate', pastDate.toISOString())).toBe(true);
  });
});
